const int no=3e6+5,modulo=1e9+7,inf=1e18,N=2e3+9;
const double PI = 3.1415926535897932384;
int ar[no],br[no],cr[no];//,dr[no],er[no];
//int used[no];
//int color[no];
//vi adj[no];
//pii adj[no];
//set<int> adj[no] ;
//LOWER_BOUND(n) - FIRST ELEMENT GREATER THAN EQUAL TO n
//UPPER_BOUND(n) - FIRST ELEMENT GREATER THAN n

//-------------------------------------------------------------------------------------------------------------------------------------------------------------- 

int dp[N][N] ;

// MEMOIZED RECURSIVE SOLUTION || BOTTOM-UP DP

int mcm(int i, int j) {
    if(i>=j) return 0;
    if(dp[i][j]!=-1) return dp[i][j] ;
    int mn=inf ;
    rep(k,i,j) {
        int tmp=mcm(i,k)+mcm(k+1,j)+ar[i]*ar[k]*ar[j] ;
        mn=min(mn,tmp) ;
    }
    return mn ;
}

void solve () {
    int n=0,m=0,a=0,b=0,c=0,d=0,x=0,y=0,z=0,w=0,k=0; 
    memset(dp,-1,sizeof(dp));
    cin>>n;
    rep(i,0,n) cin>>ar[i] ;
    cout<<mcm(1,n-1);
}

inline void runn() {
    #ifndef ONLINE_JUDGE                 
    freopen("input.txt","r",stdin);      
    freopen("output.txt","w",stdout); 
    freopen("error.txt","w",stderr);   
    #endif                               
}                                        
                                         
signed main() {                          
    night_rider
    runn(); 
    int t=1;
    cout<<fixed<<setprecision(15);
  //  cin>>t;
    rep(i,1,t+1) {
       // cout << "Case #" << i << ": ";
        solve();
    }
}
